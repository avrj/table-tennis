  "private": true,
  "dependencies": {
    "async-mqtt": "2.3.0",
    "mqtt": "3.0.0",
    "react": "16.8.6",
    "react-compound-timer": "1.1.1",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1",
    "socket.io-client": "2.2.0"
  },
  "scripts": {
